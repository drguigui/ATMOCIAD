	Month = {oct},
	Owner = {gg},
	Timestamp = {2006.12.20},
	author = {Slanger, T. G. and Huestis, D. L. and Cosby, P. C. and Bida, T. A.},
	title = {{First Observation of the 5577 \AA~Oxygen Green Line in the Night Airglow of Venus}},
	pages = {1120},
